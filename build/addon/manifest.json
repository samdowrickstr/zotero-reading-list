{
  "manifest_version": 2,
  "name": "Zotero Reading List",
  "version": "1.5.10",
  "description": "Keep track of whether you've read items in Zotero",
  "homepage_url": "https://github.com/Dominic-DallOsto/zotero-reading-list",
  "author": "Dominic Dall'Osto",
  "icons": {
    "48": "chrome/content/icons/favicon.png",
    "96": "chrome/content/icons/favicon@2x.png"
  },
  "applications": {
    "zotero": {
      "id": "reading-list@hotmail.com",
      "update_url": "https://github.com/Dominic-DallOsto/zotero-reading-list/releases/download/release/update.json",
      "strict_min_version": "7.0.9.999",
      "strict_max_version": "7.1.*"
    }
  }
}
